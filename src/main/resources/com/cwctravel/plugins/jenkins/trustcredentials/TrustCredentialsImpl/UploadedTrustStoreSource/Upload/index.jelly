<?jelly escape-by-default='true'?>
<j:jelly xmlns:j="jelly:core" xmlns:f="/lib/form" xmlns:st="jelly:stapler" xmlns:l="/lib/layout" >
  <l:layout norefresh="true">
    <l:main-panel>
      <h1>${%Upload JKS truststore}</h1>
      <f:form action="upload" method="post" name="upload">
        <f:entry>
          <input type="file" name="truststore.file" size="40"/>
        </f:entry>
        <f:block>
          <f:submit value="${%Upload}"/>
          <st:nbsp/>
          <input type="button" value="${%Cancel}"
                 onclick="window.close(); return false;"
                 class="submit-button"/>
        </f:block>
      </f:form>
    </l:main-panel>
  </l:layout>
</j:jelly>